
@{
    ViewData["Title"] = "FindUnit";
}

<h2>Find the Unit</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="FindUnit">
           
            <div class="form-group">
                <label  class="control-label">SerialNumber</label>
                <input type="text" name="readerString"  class="form-control" />
            </div>
           
            <div class="form-group">
                <input type="submit" value="FindUnit" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
